{
  "name": "setup-specmatic",
  "version": "1.7.4",
  "private": true,
  "description": "Setup specmatic action",
  "homepage": "https://github.com/marketplace/actions/setup-specmatic-environment",
  "bugs": {
    "url": "https://github.com/airslate-oss/setup-specmatic/issues"
  },
  "main": "lib/setup-specmatic.js",
  "scripts": {
    "build": "tsc && ncc build -o dist/setup src/setup-specmatic.ts",
    "format": "prettier --no-error-on-unmatched-pattern --config ./.prettierrc.js --write \"**/*.{ts,yml,yaml}\"",
    "format-check": "prettier --no-error-on-unmatched-pattern --config ./.prettierrc.js --check \"**/*.{ts,yml,yaml}\"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "jest --coverage",
    "all": "npm run format && npm run lint:fix && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airslate-oss/setup-specmatic.git"
  },
  "keywords": [
    "actions",
    "specmatic",
    "setup"
  ],
  "author": "airSlate, Inc.",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/exec": "^3.0.0",
    "@actions/http-client": "^3.0.2",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^3.0.1",
    "semver": "^7.7.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.11",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.55.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^29.13.0",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "jest": "^29.7.0",
    "jest-circus": "^30.2.0",
    "nock": "^13.5.6",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.8.3"
  }
}
