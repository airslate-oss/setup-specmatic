{
  "name": "setup-specmatic",
  "version": "1.1.0",
  "private": true,
  "description": "Setup specmatic action",
  "homepage": "https://github.com/marketplace/actions/setup-specmatic-environment",
  "bugs": {
    "url": "https://github.com/airslate-oss/setup-specmatic/issues"
  },
  "main": "lib/setup-specmatic.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '{,!(node_modules)/**/}*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airslate-oss/setup-specmatic.git"
  },
  "keywords": [
    "actions",
    "specmatic",
    "setup"
  ],
  "author": "airSlate, Inc.",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^18.11.0",
    "@types/semver": "^6.0.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vercel/ncc": "^0.36.0",
    "jest": "^27.2.5",
    "jest-circus": "^27.2.5",
    "eslint": "^8.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^27.2.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.3",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
